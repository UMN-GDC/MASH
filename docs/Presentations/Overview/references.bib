	keywords = {Schizophrenia, Functional magnetic resonance imaging, Magnetic resonance imaging, Neuroimaging, Autism spectrum disorder, Data visualization, Depression, Telecommunications},
	file = {Full Text PDF:/home/christian/snap/zotero-snap/common/Zotero/storage/ZHVWAW54/Yamashita et al. - 2019 - Harmonization of resting-state functional MRI data.pdf:application/pdf},
}

