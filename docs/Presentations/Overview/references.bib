	keywords = {{MEDLINE}, National Center for Biotechnology Information, National Institutes of Health, National Library of Medicine, {NCBI}, {NIH}, {NLM}, {PubMed} Abstract, Adolescent, Adult, Brain / physiopathology*, Child, Damien A Fair, doi:10.1016/j.dcn.2017.01.010, Emotions, Female, Humans, Magnetic Resonance Imaging / methods*, Male, Marc D Rudolph, Oscar Miranda-Domínguez, {PMC}5849238, pmid:28279917, Risk, Young Adult},
