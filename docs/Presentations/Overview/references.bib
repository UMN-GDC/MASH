	author = {Elliott, Lloyd T. and Sharp, Kevin and Alfaro-Almagro, Fidel and Shi, Sinan and Miller, Karla L. and Douaud, Gwenaëlle and Marchini, Jonathan and Smith, Stephen M.},
	date = {2018-10},
	note = {{UKB} {GWAS}},
	keywords = {Genetics of the nervous system, Genome, Quantitative trait, wide association studies},
	file = {Elliott et al. - 2018 - Genome-wide association studies of brain imaging phenotypes in UK Biobank:/home/christian/snap/zotero-snap/common/Zotero/storage/ML7KTCHS/Elliott et al. - 2018 - Genome-wide association studies of brain imaging phenotypes in UK Biobank.pdf:application/pdf},
}

@article{seal_efficient_2022,
	title = {Efficient estimation of {SNP} heritability using Gaussian predictive process in large scale cohort studies},
	volume = {18},
	issn = {1111111111},
	url = {https://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1010151},
	doi = {10.1371/JOURNAL.PGEN.1010151},
	abstract = {With the advent of high throughput genetic data, there have been attempts to estimate heritability from genome-wide {SNP} data on a cohort of distantly related individuals using linear mixed model ({LMM}). Fitting such an {LMM} in a large scale cohort study, however, is tremendously challenging due to its high dimensional linear algebraic operations. In this paper, we propose a new method named {PredLMM} approximating the aforementioned {LMM} motivated by the concepts of genetic coalescence and Gaussian predictive process. {PredLMM} has substantially better computational complexity than most of the existing {LMM} based methods and thus, provides a fast alternative for estimating heritability in large scale cohort studies. Theoretically, we show that under a model of genetic coalescence, the limiting form of our approximation is the celebrated predictive process approximation of large Gaussian process likelihoods that has well-established accuracy standards. We illustrate our approach with extensive simulation studies and use it to estimate the heritability of multiple quantitative traits from the {UK} Biobank cohort.},
	pages = {e1010151--e1010151},
	number = {4},
	journaltitle = {{PLOS} Genetics},
	author = {Seal, Souvik and Datta, Abhirup and Basu, Saonli},
	editor = {Lippert, Christoph},
	date = {2022-04},
	note = {{PredLMM}},
	keywords = {Genetics, Algorithms, Approximation methods, Cohort studies, Computer software, Heredity, Simulation and modeling, Single nucleotide polymorphisms},
	file = {Seal Id, Datta Id, Id - 2022 - Efficient estimation of SNP heritability using Gaussian predictive process in large scale cohort studies:/home/christian/snap/zotero-snap/common/Zotero/storage/4INN7SW4/Seal Id, Datta Id, Id - 2022 - Efficient estimation of SNP heritability using Gaussian predictive process in large scale cohort studies.pdf:application/pdf},
}

@article{ge_phenome-wide_2017,
