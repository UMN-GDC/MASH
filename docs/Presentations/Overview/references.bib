	file = {Mwangi, Tian, Soares - 2014 - A review of feature reduction techniques in neuroimaging:/home/christian/snap/zotero-snap/common/Zotero/storage/LGNF99JT/Mwangi, Tian, Soares - 2014 - A review of feature reduction techniques in neuroimaging.pdf:application/pdf},
