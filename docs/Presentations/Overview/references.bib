@article{grace_incorporating_2019,
	title = {Incorporating qualitative methodologies and fieldwork into large scale, quantitative analyses of climate health in low-income countries},
	volume = {3},
	url = {www.thelancet.com/planetary-health},
	doi = {10.1016/S2542-5196(19)30240-2},
	abstract = {Climate can affect individual health through a number of mechanisms, including changes in food access or availability, heat stress, and drought. Each of these mechanisms are mediated by decisions that individuals, households, and communities make, and so associations between climate change and health outcomes that do not account for these pathways can disguise true effects. In a recent study examining the link between climate variables and health in Mali and Kenya, authors found compelling evidence that precipitation, temperature, and vegetation all have an effect on birthweight. 1 A similar study examined how the climatic characteristics of a region affected growth trajectories of children under five years of age, including potentially moderating variables like transportation and health infrastructure. 2 Both studies note that although rainfall during key periods is typically associated with positive growth trajectories, quantitative associations are heterogeneous across individuals. Moreover, the associations are not fully explained after controlling for variation due to land use, food availability, infrastructure, or individual-level or household-level characteristics. Climate-health researchers have attributed the unexplained variation to a range of factors, from the adoption of modern agricultural techniques, to transportation and health infrastructure, to nutrition and care practices. The pathways proposed in quantitative climate-health studies generally come from qualitative assessments of context made by the researchers, but the directionality of those pathways is not always clear. 3,4 For instance, studies focused on investigating the association between malnutrition and climate propose that the relationship between weather conditions and child health outcomes can be partially explained by the ways a caretaker's workload changes in response to weather. In theory, children are healthier when caretakers have adequate time to care for the children. 5 In practice, increased rainfall could result in greater crop yields, but create more demand for a caretaker's labor, leaving children to eat less frequently. 6,7 Equally, dry conditions and high temperatures could result in more time on heatwave mitigation efforts and less time focused on childcare. It is also possible that extreme temperatures force individuals to spend more time indoors or close to home, resulting in more time left for caretaking. This example shows how individuals can have different responses to the same extreme weather conditions, resulting in varying health outcomes. Because responses to climate are highly contextual, quantitative analyses that do not explicitly model relevant aspects of the local context can produce misleading results. To advance climate-health research and associated policies, the research community should focus on identifying, isolating, and understanding the different ways that individuals respond to climate events. Strictly quantitative analyses have provided promising insights into climate-health linkages but are limited in their ability to capture experiences relevant to local contexts and cultures. 8 To capture variability in these dynamics, attention must be paid to local norms surrounding the ways that people interact and spend their time. Local stakeholders (all individuals in an area, including those living in the communities of interest as well as local policymakers, health workers, and educators) manage complex risks in everyday practice, and using available resources and risk mitigation strategies can provide valuable insights into research. These local stakeholders can provide unique perspectives on a range of highly relevant behavioural responses, including those related to time management, food rationing, caretaking practices, and other strategies used to reduce the adverse effects of climate change. Accessing and chronicling this information allows researchers and policy makers to better determine the ways a household and community respond to aberrant climate events. Unfortunately, standard demographic and health surveys used for climate-health research rarely capture local norms surrounding the ways that people interact. To remedy this, we suggest augmenting climate-health research with qualitative methodologies and field},
	pages = {e496--e498},
	journaltitle = {The Lancet Planetary Health},
	author = {Grace, Kathryn and Mikal, Jude},
	date = {2019-12},
	file = {Grace, Mikal - 2019 - Comment Incorporating qualitative methodologies and fieldwork into large scale, quantitative analyses of climate h:/home/christian/snap/zotero-snap/common/Zotero/storage/JX959FZU/Grace, Mikal - 2019 - Comment Incorporating qualitative methodologies and fieldwork into large scale, quantitative analyses of climate h.pdf:application/pdf},
}

@article{erhardt_comparison_2011,
