@article{fortin_harmonization_2018,
	title = {Harmonization of cortical thickness measurements across scanners and sites},
	volume = {167},
	issn = {1053-8119},
	url = {https://www.sciencedirect.com/science/article/pii/S105381191730931X},
	doi = {10.1016/j.neuroimage.2017.11.024},
	abstract = {With the proliferation of multi-site neuroimaging studies, there is a greater need for handling non-biological variance introduced by differences in {MRI} scanners and acquisition protocols. Such unwanted sources of variation, which we refer to as “scanner effects”, can hinder the detection of imaging features associated with clinical covariates of interest and cause spurious findings. In this paper, we investigate scanner effects in two large multi-site studies on cortical thickness measurements across a total of 11 scanners. We propose a set of tools for visualizing and identifying scanner effects that are generalizable to other modalities. We then propose to use {ComBat}, a technique adopted from the genomics literature and recently applied to diffusion tensor imaging data, to combine and harmonize cortical thickness values across scanners. We show that {ComBat} removes unwanted sources of scan variability while simultaneously increasing the power and reproducibility of subsequent statistical analyses. We also show that {ComBat} is useful for combining imaging data with the goal of studying life-span trajectories in the brain.},
	pages = {104--120},
	journaltitle = {{NeuroImage}},
	shortjournal = {{NeuroImage}},
	author = {Fortin, Jean-Philippe and Cullen, Nicholas and Sheline, Yvette I. and Taylor, Warren D. and Aselcioglu, Irem and Cook, Philip A. and Adams, Phil and Cooper, Crystal and Fava, Maurizio and {McGrath}, Patrick J. and {McInnis}, Melvin and Phillips, Mary L. and Trivedi, Madhukar H. and Weissman, Myrna M. and Shinohara, Russell T.},
	urldate = {2022-12-08},
	date = {2018-02-15},
	langid = {english},
	keywords = {Cortical thickness, Multi-site, {ComBat}, Harmonization, Inter-scanner},
	file = {ScienceDirect Full Text PDF:/home/christian/snap/zotero-snap/common/Zotero/storage/B6LUCQZM/Fortin et al. - 2018 - Harmonization of cortical thickness measurements a.pdf:application/pdf;ScienceDirect Snapshot:/home/christian/snap/zotero-snap/common/Zotero/storage/R72H2VMC/S105381191730931X.html:text/html},
}

@article{radua_increased_2020,
	title = {Increased power by harmonizing structural {MRI} site differences with the {ComBat} batch adjustment method in {ENIGMA}},
	volume = {218},
	issn = {1053-8119},
	url = {https://www.sciencedirect.com/science/article/pii/S1053811920304420},
	doi = {10.1016/j.neuroimage.2020.116956},
	abstract = {A common limitation of neuroimaging studies is their small sample sizes. To overcome this hurdle, the Enhancing Neuro Imaging Genetics through Meta-Analysis ({ENIGMA}) Consortium combines neuroimaging data from many institutions worldwide. However, this introduces heterogeneity due to different scanning devices and sequences. {ENIGMA} projects commonly address this heterogeneity with random-effects meta-analysis or mixed-effects mega-analysis. Here we tested whether the batch adjustment method, {ComBat}, can further reduce site-related heterogeneity and thus increase statistical power. We conducted random-effects meta-analyses, mixed-effects mega-analyses and {ComBat} mega-analyses to compare cortical thickness, surface area and subcortical volumes between 2897 individuals with a diagnosis of schizophrenia and 3141 healthy controls from 33 sites. Specifically, we compared the imaging data between individuals with schizophrenia and healthy controls, covarying for age and sex. The use of {ComBat} substantially increased the statistical significance of the findings as compared to random-effects meta-analyses. The findings were more similar when comparing {ComBat} with mixed-effects mega-analysis, although {ComBat} still slightly increased the statistical significance. {ComBat} also showed increased statistical power when we repeated the analyses with fewer sites. Results were nearly identical when we applied the {ComBat} harmonization separately for cortical thickness, cortical surface area and subcortical volumes. Therefore, we recommend applying the {ComBat} function to attenuate potential effects of site in {ENIGMA} projects and other multi-site structural imaging work. We provide easy-to-use functions in R that work even if imaging data are partially missing in some brain regions, and they can be trained with one data set and then applied to another (a requirement for some analyses such as machine learning).},
	pages = {116956},
	journaltitle = {{NeuroImage}},
	shortjournal = {{NeuroImage}},
	author = {Radua, Joaquim and Vieta, Eduard and Shinohara, Russell and Kochunov, Peter and Quidé, Yann and Green, Melissa J. and Weickert, Cynthia S. and Weickert, Thomas and Bruggemann, Jason and Kircher, Tilo and Nenadić, Igor and Cairns, Murray J. and Seal, Marc and Schall, Ulrich and Henskens, Frans and Fullerton, Janice M. and Mowry, Bryan and Pantelis, Christos and Lenroot, Rhoshel and Cropley, Vanessa and Loughland, Carmel and Scott, Rodney and Wolf, Daniel and Satterthwaite, Theodore D. and Tan, Yunlong and Sim, Kang and Piras, Fabrizio and Spalletta, Gianfranco and Banaj, Nerisa and Pomarol-Clotet, Edith and Solanes, Aleix and Albajes-Eizagirre, Anton and Canales-Rodríguez, Erick J. and Sarro, Salvador and Di Giorgio, Annabella and Bertolino, Alessandro and Stäblein, Michael and Oertel, Viola and Knöchel, Christian and Borgwardt, Stefan and du Plessis, Stefan and Yun, Je-Yeon and Kwon, Jun Soo and Dannlowski, Udo and Hahn, Tim and Grotegerd, Dominik and Alloza, Clara and Arango, Celso and Janssen, Joost and Díaz-Caneja, Covadonga and Jiang, Wenhao and Calhoun, Vince and Ehrlich, Stefan and Yang, Kun and Cascella, Nicola G. and Takayanagi, Yoichiro and Sawa, Akira and Tomyshev, Alexander and Lebedeva, Irina and Kaleda, Vasily and Kirschner, Matthias and Hoschl, Cyril and Tomecek, David and Skoch, Antonin and van Amelsvoort, Therese and Bakker, Geor and James, Anthony and Preda, Adrian and Weideman, Andrea and Stein, Dan J. and Howells, Fleur and Uhlmann, Anne and Temmingh, Henk and López-Jaramillo, Carlos and Díaz-Zuluaga, Ana and Fortea, Lydia and Martinez-Heras, Eloy and Solana, Elisabeth and Llufriu, Sara and Jahanshad, Neda and Thompson, Paul and Turner, Jessica and van Erp, Theo and Glahn, David and Pearlson, Godfrey and Hong, Elliot and Krug, Axel and Carr, Vaughan and Tooney, Paul and Cooper, Gavin and Rasser, Paul and Michie, Patricia and Catts, Stanley and Gur, Raquel and Gur, Ruben and Yang, Fude and Fan, Fengmei and Chen, Jingxu and Guo, Hua and T